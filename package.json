{
  "name": "numcodecs",
  "author": "Trevor James Manz",
  "version": "0.1.1",
  "description": "Buffer compression and transformation codecs for use in data storage and communication applications.",
  "types": "dist/types/src/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./gzip": {
      "import": "./dist/gzip.mjs",
      "require": "./dist/gzip.cjs"
    },
    "./zlib": {
      "import": "./dist/zlib.mjs",
      "require": "./dist/zlib.cjs"
    },
    "./blosc": {
      "import": "./dist/blosc.mjs",
      "require": "./dist/blosc.cjs"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/manzt/numcodecs.js.git"
  },
  "scripts": {
    "build": "rollup -c",
    "prebuild": "rm -rf dist",
    "format": "prettier --write src/**/*.ts test/**/*.js",
    "lint": "prettier --check src src/**/*.ts test/**/*.js",
    "test": "npm run build && node test/index.test.js",
    "test:prod": "npm run lint && npm test"
  },
  "keywords": [
    "zarr",
    "compression",
    "codecs",
    "storage"
  ],
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/pako": "^1.0.1",
    "prettier": "2.1.1",
    "rollup": "^2.26.9",
    "rollup-plugin-terser": "^7.0.1",
    "typescript": "^4.0.2",
    "zora": "^4.0.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "dependencies": {
    "pako": "^1.0.11"
  }
}
